# Dreamwalker MCP Environment Variables
# Copy this to ~/.env and fill in your API keys

# Core LLM Provider Keys (at least one required for orchestration)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
XAI_API_KEY=
MISTRAL_API_KEY=
COHERE_API_KEY=
GEMINI_API_KEY=
PERPLEXITY_API_KEY=
GROQ_API_KEY=
HUGGINGFACE_API_KEY=

# Data Source Keys (optional, enables specific data tools)
YOUTUBE_API_KEY=
GITHUB_TOKEN=
NASA_API_KEY=
NEWS_API_KEY=
OPENWEATHER_API_KEY=
ALPHA_VANTAGE_API_KEY=
CENSUS_API_KEY=

# Search Provider Keys (at least one required for web search)
SERP_API_KEY=
BRAVE_API_KEY=

# Redis Configuration (defaults shown)
REDIS_HOST=localhost
REDIS_PORT=6379
EOF < /dev/null
